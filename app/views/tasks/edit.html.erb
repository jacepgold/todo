<h1>Edit Task</h1>
<%= form_for @task do |f| %>
  <%= f.label :title %>
  <br />
  <%= f.text_field :title %>
  <br />
  <br />
  <%= f.label :desription %>
  <br />
  <%= f.text_area :description %>
  <br />
  <%= f.submit %>
<% end %>
<br />
<%= link_to 'Complete Task', @task, method: :delete %>
<%= link_to 'View Tasks', tasks_path %>